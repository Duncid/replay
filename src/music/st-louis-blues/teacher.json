  "motifs": [
    {
      "id": "M1",
      "label": "Pickup-to-long-hold (tie into sustain)",
      "importance": 0.7,
      "description": "Short pickup gesture that resolves into a long held tone; keep the sustain steady and counted."
    },
    {
      "id": "M2",
      "label": "Chromatic approach (C–C–C#–D) into hold",
      "importance": 0.85,
      "description": "Bluesy chromatic lift into a target tone; keep the rhythm even and the approach expressive."
    },
    {
      "id": "M3",
      "label": "A-theme cell (B–D–B–G) with tie",
      "importance": 0.9,
      "description": "Main melodic hook shape: up to D then down to G with a held resolution."
    },
    {
      "id": "M4",
      "label": "Verse pickup (E–G–A) into long A",
      "importance": 0.75,
      "description": "Light pickup into a tied/held A; avoid rushing the pickup."
    },
    {
      "id": "M5",
      "label": "Tag pickup (A#–B)",
      "importance": 0.6,
      "description": "Two-note pickup that launches the next phrase/repeat; land cleanly on B."
    }
  ],
